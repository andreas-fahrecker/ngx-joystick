{
  "name": "ngx-joystick",
  "version": "1.0.7",
  "description": "NgxJoystick is an angular joystick component. It is a wrapper around the nipplejs library providing virtual joystick functionality to your angular application.",
  "author": "Wes Weitzel",
  "repository": {
    "type": "git",
    "url": "https://github.com/wweitzel/ngx-joystick.git"
  },
  "keywords": [
    "angular",
    "javascript",
    "typescript",
    "component",
    "joystick",
    "nipplejs"
  ],
  "license": "MIT",
  "dependencies": {
    "nipplejs": "^0.9.1",
    "tslib": "^2.4.0"
  },
  "peerDependencies": {
    "@angular/common": "~14.1.1",
    "@angular/core": "~14.1.1"
  },
  "module": "fesm2015/ngx-joystick.mjs",
  "es2020": "fesm2020/ngx-joystick.mjs",
  "esm2020": "esm2020/ngx-joystick.mjs",
  "fesm2020": "fesm2020/ngx-joystick.mjs",
  "fesm2015": "fesm2015/ngx-joystick.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2020": "./esm2020/ngx-joystick.mjs",
      "es2020": "./fesm2020/ngx-joystick.mjs",
      "es2015": "./fesm2015/ngx-joystick.mjs",
      "node": "./fesm2015/ngx-joystick.mjs",
      "default": "./fesm2020/ngx-joystick.mjs"
    }
  },
  "sideEffects": false
}